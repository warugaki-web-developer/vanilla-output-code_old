{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[nunjucks]": {
    "editor.defaultFormatter": "okitavera.vscode-nunjucks-formatter"
  },
  "files.associations": {
    "*.njk": "html",
    "**/robots.txt.njk": "nunjucks"
  },
  "emmet.includeLanguages": {
    "njk": "html",
    "nunjucks": "html"
  }
}
